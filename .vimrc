execute pathogen#infect()
set nocompatible
filetype plugin indent on
set expandtab
set tabstop=2
set softtabstop=2
set shiftwidth=2
set t_u7=
"set relativenumber
set number
syntax on
nnoremap <leader>n :NERDTreeFocus<CR>
nnoremap <C-n> :NERDTreeToggle<CR>
nnoremap <leader>r :set relativenumber!<CR>:set relativenumber?<CR>
